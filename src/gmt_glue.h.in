/* gmt_shared.h declares the prototypes for shared library function
 * calls that the gmt driver needs to find in order to learn about
 * each module in the shared library.  This include file is included
 * by gmt_modules.h.
 */


/* Pretty print all modules in the shared library and their purposes */
EXTERN_MSC void gmtlib_@SUPPL_NAME@_module_show_all (void *API);
/* List all modern modules in the shared library to stdout */
EXTERN_MSC void gmtlib_@SUPPL_NAME@_module_list_all (void *API);
/* List all classic modules in the shared library to stdout */
EXTERN_MSC void gmtlib_@SUPPL_NAME@_module_classic_all (void *API);
/* Function called by GMT_Encode_Options so developers can get information about a module */
EXTERN_MSC const char * gmtlib_@SUPPL_NAME@_module_keys (void *API, char *candidate);
/* Function returns name of group that module belongs to */
EXTERN_MSC const char * gmtlib_@SUPPL_NAME@_module_group (void *API, char *candidate);
